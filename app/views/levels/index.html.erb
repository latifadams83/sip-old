<p id="notice"><%= notice %></p>

<h4>Levels</h4>

<table class="w3-table-all">
  <thead>
    <tr class="w3-theme-d4">
      <th>Level</th>
      <th>Name</th>
      <th class="w3-center"> Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @levels.each do |level| %>
      <tr>
        <td><%= level.lev %></td>
        <td><%= level.name %></td>
        <td class="w3-center">
          <%= link_to 'Edit', edit_level_path(level), class: "w3-btn w3-round w3-tiny w3-amber" %>
          <%= link_to "Del", level, method: :delete, data: { confirm: 'Are you sure?' }, class: "w3-btn w3-round w3-tiny w3-red" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to '+', new_level_path, class: "w3-btn w3-round w3-theme-d4 w3-right w3-tiny" %>
