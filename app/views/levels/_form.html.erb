<div class="w3-container w3-theme-l4 small-form">
  <%= form_for(level, url: url) do |f| %>
  <%= error_messages_for(f.object)%>

    <div class="field">
      <br>

      <%= f.label :lev, :Level, class: "w3-label"%><br>
      <%= f.select(:lev, 1..10, {:include_blank => true}, { :class => "w3-input" }) %>
    </div>
    <br>
    <div class="field">
      <%= f.label :name, class: "w3-label" %>
      <%= f.text_field :name, class: "w3-input" %>
    </div>
    <br>
    <div class="actions">
      <%= f.submit :Submit, class: "w3-btn w3-round w3-theme-d4 w3-right" %>

  <% end %>

</div>
