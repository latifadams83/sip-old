<p id="notice"><%= notice %></p>

<h1>Find Students</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Student</th>
      <th>Programme</th>
      <th>Level</th>
      <th>Badge</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @find_students.each do |find_student| %>
      <tr>
        <td><%= find_student.first_name %></td>
        <td><%= find_student.last_name %></td>
        <td><%= find_student.student_id %></td>
        <td><%= find_student.programme_id %></td>
        <td><%= find_student.level_id %></td>
        <td><%= find_student.badge_id %></td>
        <td><%= find_student.status %></td>
        <td><%= link_to 'Show', find_student %></td>
        <td><%= link_to 'Edit', edit_find_student_path(find_student) %></td>
        <td><%= link_to 'Destroy', find_student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Find Student', new_find_student_path %>
