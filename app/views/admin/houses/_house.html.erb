<tr>
        <td><%= house.name %></td>
        <td><%= house.capacity %></td>
        <td>
          <td class="w3-center">
          <span>
            <%= link_to "#{content_tag(:i, '', :class => ("fa fa-eye"))}".html_safe, admin_house_path(house) %>&nbsp;&nbsp;
            <%= link_to "#{content_tag(:i, '', :class => ("fa fa-pencil"))}".html_safe, edit_admin_house_path(house)%>&nbsp;&nbsp;
            <%= link_to "#{content_tag(:i, '', :class => ("fa fa-trash"))}".html_safe, admin_house_path(house), method: :delete, data: { confirm: 'Are you sure?' } %>
          </span>
          </td>
        </td>
      </tr>