  <%= form_for(level, url: url) do |f| %>
  <%= error_messages_for(f.object)%>

    <div class="w3-row w3-padding-top">
      <div class="w3-col s12">
        <%= f.label :level, :Level, class: "w3-label"%><br>
        <%= f.number_field :level, class: "w3-input w3-card w3-round", min: "1" %>
      </div>
    </div>
    <div class="w3-row w3-padding-top">
      <div class="w3-col s12">
        <%= f.label :name, class: "w3-label" %>
        <%= f.text_field :name, class: "w3-input w3-card w3-round" %>
      </div>
    </div>
    <br>
    <div class="w3-row w3-padding-top">
      <%= f.submit :Submit, class: "w3-btn w3-round w3-theme-d4 w3-right" %>

  <% end %>
