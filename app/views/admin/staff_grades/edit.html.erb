<%= provide(:page_title, "Edit Staff Grade")%>
<%= provide(:staff_active, "activelink")%>

  <fieldset class="w3-panel w3-round-large w3-card-2 w3-light-gray w3-small">
    <legend class="w3-container w3-padding">
      <a href="javascript:void();" onclick="$('#editGrade').toggle(300);"><b>Edit Staff Grade</b> <i class="fa fa-caret-down"></i></a>
    </legend>
  <div id="editGrade">
    <%= render 'form', staff_grade: @staff_grade, url: admin_staff_grade_url(@staff_grade) %>
    <%= link_to 'Cancel', admin_staff_grades_path, class: "w3-btn w3-round" %>
    </div>
  </div>
  </fieldset>
