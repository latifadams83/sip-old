<%= provide(:page_title, "New Staff Grade")%>
<%= provide(:staff_active, "activelink")%>

  <fieldset class="w3-panel w3-round-large w3-card-2 w3-light-gray w3-small">
    <legend class="w3-container w3-padding">
      <a href="javascript:void();" onclick="$('#newGrade').toggle(300);"><b>New Staff Grade</b> <i class="fa fa-caret-down"></i></a>
    </legend>
  <div id="newGrade">
    <%= render 'form', staff_grade: @staff_grade, url: admin_staff_grades_path %>
    <%= link_to 'Cancel', admin_staff_grades_path, class: "w3-btn w3-round w3-left w3-margin-right " %>
    </div>
  </div>
  </fieldset>
