<%= form_for(programme, url: url) do |f| %>
  <%= error_messages_for(f.object)%>
    <div class="w3-row w3-padding-top">
      <div class="w3-col s12">
        <%= f.label :name, class: "w3-label" %>
        <%= f.text_field :name, class: "w3-input w3-border w3-round "%>
      </div>
    </div>
    <div class="w3-row w3-padding-top">
      <div class="w3-col s12">
        <%= f.label :code, class: "w3-label" %>
        <%= f.text_field :code, class: "w3-input w3-border w3-round" %>
      </div>
    </div>
    <br>
    <div>
      <%= f.submit :Submit,  class: "w3-btn w3-right w3-theme-d1 w3-round w3-margin-bottom " %>
<% end %>
