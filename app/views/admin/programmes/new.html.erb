<%= provide(:page_title, "New Programme")%>
<%= provide(:settings_active, "activelink")%>


<fieldset class="w3-input-group w3-round w3-theme-l4 w3-margin-bottom">
  <legend class="w3-container w3-white">New Programme</legend>

  <div class="w3-round w3-theme-l4">
    <%= render 'form', programme: @programme, url: admin_programmes_url %>
    <%= link_to 'Cancel', admin_programmes_path, class: "w3-btn w3 w3-round w3-left w3-margin-bottom" %>

    </div>

  </div>

</fieldset>
