<%= provide(:page_title, "Edit Levl")%>
<%= provide(:settings_active, "activelink")%>

<fieldset class="w3-input-group w3-round w3-theme-l4 w3-margin-bottom">
  <legend class="w3-container w3-white">Level Details</legend>
  <div class="w3-round w3-theme-l4">
    <%= render 'form', level: @level, url: admin_level_path(@level) %>
    <%= link_to 'Cancel', admin_levels_path, class: "w3-btn w3 w3-round w3-left w3-margin-bottom" %>

  </div>
</div>

</fieldset>
